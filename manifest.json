{
    "manifest_version": 3,
    "name": "Momentum Clone",
    "version": "1.0",
    "description": "A new tab extension that displays time, greeting, and other productivity features.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "popup.html"
    },
    "icons": {
      "16": "assets/icon.png",
      "48": "assets/icon.png",
      "128": "assets/icon.png"
    }
  }
  